{
  "name": "@veramo/message-handler",
  "description": "Message Handler plugin for Veramo",
  "version": "5.6.0",
  "main": "build/index.cjs.js",  
  "module": "build/index.js",
  "types": "build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js",
      "require": "./build/index.cjs.js"
    }
  },
  "scripts": {
    "build": "node ../../scripts/esbuild.js",
    "extract-api": "node --experimental-specifier-resolution=node ../cli/bin/veramo.js dev extract-api"
  },
  "dependencies": {
    "@veramo/core-types": "workspace:^",
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "@types/debug": "4.1.8",
    "typescript": "5.2.2"
  },
  "files": [
    "build/**/*",
    "src/**/*",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/decentralized-identity/veramo.git",
    "directory": "packages/message-handler"
  },
  "author": "Consensys Mesh R&D <hello@veramo.io>",
  "contributors": [
    "Simonas Karuzas <simonas.karuzas@mesh.xyz>",
    "Mircea Nistor <mircea.nistor@mesh.xyz>"
  ],
  "keywords": [
    "Veramo",
    "DID",
    "Verifiable Credential",
    "veramo-plugin",
    "aggregator",
    "messaging",
    "DIDComm"
  ],
  "license": "Apache-2.0",
  "type": "module",
  "moduleDirectories": [
    "node_modules",
    "src"
  ]
}
